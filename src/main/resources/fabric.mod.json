{
	"schemaVersion": 1,
	"id": "icu_bundles",
	"version": "${version}",
	"name": "I Can't Update - Bundles of Bravery",
	"description": "Backports most of the new bundle features to 1.20.1 for those who can't update cause of other mods",
	"authors": [
		"Pluto7073"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Pluto7073/plutos-example-mod"
	},
	"license": "MIT",
	"icon": "assets/icu_bundles/icon.png",
	"accesswidener": "icu_bundles.accesswidener",
	"environment": "*",
	"entrypoints": {
		"main": [
			"ml.pluto7073.icu.bundles.BundlesOfBravery"
		],
		"client": [
			"ml.pluto7073.icu.bundles.client.BundlesOfClient"
		]
	},
	"mixins": [
		"icu_bundles.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": "~${minecraft_version}",
		"java": ">=17",
		"fabric-api": ">=${fabric_api_version}"
	},
	"suggests": {
		"another-mod": "*"
	}
}